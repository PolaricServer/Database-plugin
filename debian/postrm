#!/bin/sh
set -e


case "$1" in
    remove)
       # Nothing to do here
    ;;

    purge)
    ;;

    upgrade|failed-upgrade|abort-install|abort-upgrade|disappear)
        # Nothing to do here
    ;;

    *)
